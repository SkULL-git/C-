#include<iostream>
using namespace std;
int main() {
    // int a = 14;
    // int b = 15;
    int a = 14, b = 17;
    float pi=3.14;
    cout<<"This is the respective values of a and b.\nHere the value of a is " << a << "\nAnd the value of b is " << b;
    cout<<"\nThe value of pi is "<<pi;
    return 0;
}
